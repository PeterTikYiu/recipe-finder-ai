# Rename this file to `.env` (for local dev) or set these in your hosting provider.
# Vite only exposes variables prefixed with VITE_.

# Spoonacular API
VITE_SPOONACULAR_API_KEY=your_spoonacular_key_here

# Firebase (optional; if omitted, the app runs with auth disabled)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_APP_ID=1:xxxxxxxxxxxx:web:yyyyyyyyyyyy
